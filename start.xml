<?xml version="1.0" encoding="UTF-8"?>
<MOML>
    <THEMES>
        <THEME id="style1" style="round" padding="10" margin="5"/>
    </THEMES>
    <UILAYOUT portrait="320,460" landscape="320,460">
        <WINDOW layout="0,0,320,460" defaultImg="#ffffff">
            <LABEL layout="10,10,auto,auto" text="Hello MOML" />
            <CAMERA id="cam" layout="10,60,300,300"/>
            <WINDOW layout="10,380,300,auto"    align="linear">
                <STYLEBUTTON themeId="style1" text="open" img="#0080ff" onClick="cam.open"/>
                <STYLEBUTTON themeId="style1" text="close" onClick="cam.close"/>
                <STYLEBUTTON themeId="style1" text="front" onClick="cam.type='front'"/>
                <STYLEBUTTON themeId="style1" text="back"  onClick="cam.type='back'"/>
                <STYLEBUTTON themeId="style1" text="takee"  onClick="cam.takePicture"/>
                
            </WINDOW>    
        
            
        </WINDOW>
    </UILAYOUT>
</MOML>
